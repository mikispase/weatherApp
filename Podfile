# platform :ios, '9.0'


def shared_pods
    pod 'AFNetworking', '= 3.0'
    pod 'SDWebImage', '~> 4.0'
    pod 'DZNEmptyDataSet'
    pod 'TSMessages'
    pod 'SVProgressHUD', :git => 'https://github.com/SVProgressHUD/SVProgressHUD.git'
end

target 'Development' do
  shared_pods

end

target 'Production' do
 shared_pods

end

target 'Test' do
   shared_pods

end



